<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <solid android:color="#FFD47878"/>
    <!--
        设置圆角   
        注意：        bottomRightRadius是左下角而不是右下角  bottomLeftRadius右下角
    -->
